{
  "name": "mix/http-server",
  "description": "Http server library based on Swoole coroutine, following PSR-7 / PSR-15 standard",
  "type": "library",
  "keywords": [
    "mix",
    "swoole",
    "http",
    "server",
    "coroutine"
  ],
  "homepage": "https://openmix.org/",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "liu,jian",
      "email": "coder.keda@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-swoole": ">=4.4.4",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "mix/http-message": "~2.2.0",
    "mix/micro-server": "~2.2.0"
  },
  "autoload": {
    "psr-4": {
      "Mix\\Http\\Server\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0.0",
    "mix/guzzle": "~2.2.0"
  }
}